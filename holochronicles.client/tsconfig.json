{
    "files": [],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [ "./src/*" ]
        },
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [ "dom", "esnext" ],
        "types": [ "vite-plugin-svgr/client" ]
    },
    "include": [ "src", "src/env.d.ts", "unittest/**/*.test.ts" ],
    "exclude": [ "node_modules" ]
}